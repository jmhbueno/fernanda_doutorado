###### Regressoes para todas as variaveis------
view(Grande_banco_riasec)
logit_g_educa <- glm(ti_g_educa ~ R+I+A+S+E+C, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_gesta <- glm(ti_g_gesta ~ R+I+A+S+E+C, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_userx <- glm(ti_g_userx ~ R+I+A+S+E+C, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_supma <- glm(ti_g_supma ~ R+I+A+S+E+C, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_dados <- glm(ti_g_dados ~ R+I+A+S+E+C, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_infra <- glm(ti_g_infra ~ R+I+A+S+E+C, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_desen <- glm(ti_g_desen ~ R+I+A+S+E+C, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)

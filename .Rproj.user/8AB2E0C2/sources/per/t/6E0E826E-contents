# copia do ti para front end
Grande_banco_riasec$ti_front <- Grande_banco_riasec$ti
view(count(Grande_banco_riasec, ti_area))
view(count(Grande_banco_riasec, ti_front))

# Primeira etapa e selecionar a profissao desejada e colocar como 2 no ti_front
Grande_banco_riasec$ti_front[Grande_banco_riasec$ti_area == 'Desenvolvimento front-end'] <- 2
# Em seguida, todos que sao de ti mas nao fazem parte da profissao especifica levam NA
Grande_banco_riasec$ti_front[Grande_banco_riasec$ti_front == 1] <- 'NA'
# Depois, o grupo especifico que era 2 sera transformado em 1 para auxiliar o calculo da regressao logistica
Grande_banco_riasec$ti_front[Grande_banco_riasec$ti_front == 2] <- 1


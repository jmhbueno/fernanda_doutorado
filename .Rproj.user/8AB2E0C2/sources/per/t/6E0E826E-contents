# Primeiro passo: calculo das médias e dos desvios padrões

mean_R  <- mean(Grande_banco_riasec$R ) %>% round(digits=2)
mean_R1 <- mean(Grande_banco_riasec$R1) %>% round(digits=2)
mean_R2 <- mean(Grande_banco_riasec$R2) %>% round(digits=2)
mean_R3 <- mean(Grande_banco_riasec$R3) %>% round(digits=2)
mean_R4 <- mean(Grande_banco_riasec$R4) %>% round(digits=2)
mean_I  <- mean(Grande_banco_riasec$I ) %>% round(digits=2)
mean_I1 <- mean(Grande_banco_riasec$I1) %>% round(digits=2)
mean_I2 <- mean(Grande_banco_riasec$I2) %>% round(digits=2)
mean_A  <- mean(Grande_banco_riasec$A ) %>% round(digits=2)
mean_A1 <- mean(Grande_banco_riasec$A1) %>% round(digits=2)
mean_A2 <- mean(Grande_banco_riasec$A2) %>% round(digits=2)
mean_S  <- mean(Grande_banco_riasec$S ) %>% round(digits=2)
mean_S1 <- mean(Grande_banco_riasec$S1) %>% round(digits=2)
mean_S2 <- mean(Grande_banco_riasec$S2) %>% round(digits=2)
mean_S3 <- mean(Grande_banco_riasec$S3) %>% round(digits=2)
mean_E  <- mean(Grande_banco_riasec$E ) %>% round(digits=2)
mean_E1 <- mean(Grande_banco_riasec$E1) %>% round(digits=2)
mean_E2 <- mean(Grande_banco_riasec$E2) %>% round(digits=2)
mean_E3 <- mean(Grande_banco_riasec$E3) %>% round(digits=2)
mean_E4 <- mean(Grande_banco_riasec$E4) %>% round(digits=2)
mean_C  <- mean(Grande_banco_riasec$C ) %>% round(digits=2)
mean_C1 <- mean(Grande_banco_riasec$C1) %>% round(digits=2)
mean_C2 <- mean(Grande_banco_riasec$C2) %>% round(digits=2)

sdev_R  <- sd(Grande_banco_riasec$R ) %>% round(digits=2)
sdev_R1 <- sd(Grande_banco_riasec$R1) %>% round(digits=2)
sdev_R2 <- sd(Grande_banco_riasec$R2) %>% round(digits=2)
sdev_R3 <- sd(Grande_banco_riasec$R3) %>% round(digits=2)
sdev_R4 <- sd(Grande_banco_riasec$R4) %>% round(digits=2)
sdev_I  <- sd(Grande_banco_riasec$I ) %>% round(digits=2)
sdev_I1 <- sd(Grande_banco_riasec$I1) %>% round(digits=2)
sdev_I2 <- sd(Grande_banco_riasec$I2) %>% round(digits=2)
sdev_A  <- sd(Grande_banco_riasec$A ) %>% round(digits=2)
sdev_A1 <- sd(Grande_banco_riasec$A1) %>% round(digits=2)
sdev_A2 <- sd(Grande_banco_riasec$A2) %>% round(digits=2)
sdev_S  <- sd(Grande_banco_riasec$S ) %>% round(digits=2)
sdev_S1 <- sd(Grande_banco_riasec$S1) %>% round(digits=2)
sdev_S2 <- sd(Grande_banco_riasec$S2) %>% round(digits=2)
sdev_S3 <- sd(Grande_banco_riasec$S3) %>% round(digits=2)
sdev_E  <- sd(Grande_banco_riasec$E ) %>% round(digits=2)
sdev_E1 <- sd(Grande_banco_riasec$E1) %>% round(digits=2)
sdev_E2 <- sd(Grande_banco_riasec$E2) %>% round(digits=2)
sdev_E3 <- sd(Grande_banco_riasec$E3) %>% round(digits=2)
sdev_E4 <- sd(Grande_banco_riasec$E4) %>% round(digits=2)
sdev_C  <- sd(Grande_banco_riasec$C ) %>% round(digits=2)
sdev_C1 <- sd(Grande_banco_riasec$C1) %>% round(digits=2)
sdev_C2 <- sd(Grande_banco_riasec$C2) %>% round(digits=2)

# Segundo passo: calculo dos percentis correspondetes às pontuações de 1 a 4
# As pontuações foram subindo com intervalos de 0,1 ponto.

p_R  <- round(pnorm(seq(from=1,by=0.1,length=31),mean_R ,sdev_R )*100,2)
p_R1 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_R1,sdev_R1)*100,2)
p_R2 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_R2,sdev_R2)*100,2)
p_R3 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_R3,sdev_R3)*100,2)
p_R4 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_R4,sdev_R4)*100,2)
p_I  <- round(pnorm(seq(from=1,by=0.1,length=31),mean_I ,sdev_I )*100,2)
p_I1 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_I1,sdev_I1)*100,2)
p_I2 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_I2,sdev_I2)*100,2)
p_A  <- round(pnorm(seq(from=1,by=0.1,length=31),mean_A ,sdev_A )*100,2)
p_A1 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_A1,sdev_A1)*100,2)
p_A2 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_A2,sdev_A2)*100,2)
p_S  <- round(pnorm(seq(from=1,by=0.1,length=31),mean_S ,sdev_S )*100,2)
p_S1 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_S1,sdev_S1)*100,2)
p_S2 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_S2,sdev_S2)*100,2)
p_S3 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_S3,sdev_S3)*100,2)
p_E  <- round(pnorm(seq(from=1,by=0.1,length=31),mean_E ,sdev_E )*100,2)
p_E1 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_E1,sdev_E1)*100,2)
p_E2 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_E2,sdev_E2)*100,2)
p_E3 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_E3,sdev_E3)*100,2)
p_E4 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_E4,sdev_E4)*100,2)
p_C  <- round(pnorm(seq(from=1,by=0.1,length=31),mean_C ,sdev_C )*100,2)
p_C1 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_C1,sdev_C1)*100,2)
p_C2 <- round(pnorm(seq(from=1,by=0.1,length=31),mean_C2,sdev_C2)*100,2)

# Tabela: a primeira coluna contem as pontuacoes; as demais contêm os respectivos percentis.

tabela_percentil_teórico <- data.frame(
  Pontuações = c(seq(from=1,by=0.1,length=31)),
  R  = p_R,
  R1 = p_R1,
  R2 = p_R2,
  R3 = p_R3,
  R4 = p_R4,
  I  = p_I ,
  I1 = p_I1,
  I2 = p_I2,
  A  = p_A ,
  A1 = p_A1,
  A2 = p_A2,
  S  = p_S ,
  S1 = p_S1,
  S2 = p_S2,
  S3 = p_S3,
  E  = p_E ,
  E1 = p_E1,
  E2 = p_E2,
  E3 = p_E3,
  E4 = p_E4,
  C  = p_C ,
  C1 = p_C1,
  C2 = p_C2
)

# Calculo dos escores percentílicos dos sujeitos

round(pnorm(Grande_banco_riasec$R,mean_R ,sdev_R )*100,2)

Grande_banco_riasec$p_R  <- round(pnorm(Grande_banco_riasec$R ,mean_R ,sdev_R )*100,2)
Grande_banco_riasec$p_R1 <- round(pnorm(Grande_banco_riasec$R1,mean_R1,sdev_R1)*100,2)
Grande_banco_riasec$p_R2 <- round(pnorm(Grande_banco_riasec$R2,mean_R2,sdev_R2)*100,2)
Grande_banco_riasec$p_R3 <- round(pnorm(Grande_banco_riasec$R3,mean_R3,sdev_R3)*100,2)
Grande_banco_riasec$p_R4 <- round(pnorm(Grande_banco_riasec$R4,mean_R4,sdev_R4)*100,2)
Grande_banco_riasec$p_I  <- round(pnorm(Grande_banco_riasec$I ,mean_I ,sdev_I )*100,2)
Grande_banco_riasec$p_I1 <- round(pnorm(Grande_banco_riasec$I1,mean_I1,sdev_I1)*100,2)
Grande_banco_riasec$p_I2 <- round(pnorm(Grande_banco_riasec$I2,mean_I2,sdev_I2)*100,2)
Grande_banco_riasec$p_A  <- round(pnorm(Grande_banco_riasec$A ,mean_A ,sdev_A )*100,2)
Grande_banco_riasec$p_A1 <- round(pnorm(Grande_banco_riasec$A1,mean_A1,sdev_A1)*100,2)
Grande_banco_riasec$p_A2 <- round(pnorm(Grande_banco_riasec$A2,mean_A2,sdev_A2)*100,2)
Grande_banco_riasec$p_S  <- round(pnorm(Grande_banco_riasec$S ,mean_S ,sdev_S )*100,2)
Grande_banco_riasec$p_S1 <- round(pnorm(Grande_banco_riasec$S1,mean_S1,sdev_S1)*100,2)
Grande_banco_riasec$p_S2 <- round(pnorm(Grande_banco_riasec$S2,mean_S2,sdev_S2)*100,2)
Grande_banco_riasec$p_S3 <- round(pnorm(Grande_banco_riasec$S3,mean_S3,sdev_S3)*100,2)
Grande_banco_riasec$p_E  <- round(pnorm(Grande_banco_riasec$E ,mean_E ,sdev_E )*100,2)
Grande_banco_riasec$p_E1 <- round(pnorm(Grande_banco_riasec$E1,mean_E1,sdev_E1)*100,2)
Grande_banco_riasec$p_E2 <- round(pnorm(Grande_banco_riasec$E2,mean_E2,sdev_E2)*100,2)
Grande_banco_riasec$p_E3 <- round(pnorm(Grande_banco_riasec$E3,mean_E3,sdev_E3)*100,2)
Grande_banco_riasec$p_E4 <- round(pnorm(Grande_banco_riasec$E4,mean_E4,sdev_E4)*100,2)
Grande_banco_riasec$p_C  <- round(pnorm(Grande_banco_riasec$C ,mean_C ,sdev_C )*100,2)
Grande_banco_riasec$p_C1 <- round(pnorm(Grande_banco_riasec$C1,mean_C1,sdev_C1)*100,2)
Grande_banco_riasec$p_C2 <- round(pnorm(Grande_banco_riasec$C2,mean_C2,sdev_C2)*100,2)


#datatable(r_itens$loadings[1:23, 1:3], rownames=TRUE, editable = TRUE) 

library(knitr)  #pacote para tabelas
kable(r_efa$loadings[1:23, 1:4],digits = 2)

#R-Fidedignidade
#names(riasec_2)
#riasec_2 %>% select(24:29,36,41:43,45) %>% alpha()

names(R)

rely_R1<-R %>% select(13:16,20,22,23) %>% omega(digits=3,poly=TRUE)
rely_R2<-R %>% select(1,3,4,6,12,18:21,23) %>% omega(digits=3,poly=TRUE)
rely_R3<-R %>% select(7:11) %>% omega(digits=3,poly=TRUE)
rely_R4<-R %>% select(2,3,5) %>% omega(digits=3,poly=TRUE,nfactors = 2)



rely_R1$alpha %>% round(digits=2)
rely_R1$omega.tot %>% round(digits=2)
rely_R2$alpha %>% round(digits=2)
rely_R2$omega.tot %>% round(digits=2)
rely_R3$alpha %>% round(digits=2)
rely_R3$omega.tot %>% round(digits=2)
rely_R4$alpha %>% round(digits=2)
rely_R4$omega.tot %>% round(digits=2)

#Cálculo das pontuações e inserção em riasec_2

riasec_3$R1 <- R %>% select(13:16,20,22,23) %>% rowMeans()
riasec_3$R2 <- R %>% select(1,3,4,6,12,18:21,23) %>% rowMeans()
riasec_3$R3<-R %>% select(7:11) %>% rowMeans()
riasec_3$R4<-R %>% select(2,3,5) %>% rowMeans()
names(riasec_3)
glimpse(riasec_3$R1)


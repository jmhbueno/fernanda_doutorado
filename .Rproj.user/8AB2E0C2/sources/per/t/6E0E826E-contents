riasec_3$A<- escores %>% select(8,9) %>% rowMeans()
riasec_3$S<- escores %>% select(6,10,11) %>% rowMeans()
riasec_3$E<- escores %>% select(12:15) %>% rowMeans()
riasec_3$C<- escores %>% select(16,17) %>% rowMeans()

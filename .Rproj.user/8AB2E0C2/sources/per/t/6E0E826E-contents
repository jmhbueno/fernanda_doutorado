#Importar banco de dados========================================================

riasec <- riasec_ponto_zero #todas as variáveis baixadas do formulário Google.
names(riasec)

riasec$TCLE[riasec$TCLE == 'Concordo em participar da pesquisa'] <- '1'
riasec$TCLE[riasec$TCLE == 'Não concordo em participar da pesquisa'] <- '0'
riasec_1 <- riasec %>% filter(TCLE==1) # Eliminação de não respondentes
riasec_2 <- riasec_1 %>% filter(Idade>=18) # Eliminação de menores de 18 anos 
riasec_2 <- riasec_2 %>% slice(10:3009)
riasec_2 <- riasec_2 %>% slice(-1471)
view(riasec_2)
riasec_3 <- riasec_2[1:2000, ] #banco de dados para análise exploratória
riasec_4 <- riasec_2[2001:2609, ] # banco de dados para análise confirmatória
view(riasec_3)
view(riasec_3_original)
R <- riasec_3 %>% select(42:65)
I <- riasec_3 %>% select(66:78,138:144,146)
A <- riasec_3 %>% select(79:91)
S <- riasec_3 %>% select(92:105)
E <- riasec_3 %>% select(106:123,145)
C <- riasec_3 %>% select(124:137)

names(R)

# frequencia de algumas variaveis
count(riasec_3, Sexo)
count(riasec_4, Sexo)
mean(x=riasec_3$Idade)
mean(x=riasec_4$Idade)

#AFE===========================================================================

# Descrição dos participantes

names(riasec_3)
describe(riasec_3$Idade) %>% kable()
describeBy(riasec_3$Idade,riasec_3$Sexo, mat=TRUE,digits=3) %>% kable()
describe(riasec_3$Idade)
esc <- riasec_3 %>% count(Escolaridade)
prop.table(esc$n)
count(riasec_3,Escolaridade)

write

# EFA REALISTICO====================================================================

#KMO

R %>% KMO()

#Bartlett

R %>% bartlett.test()

# Paralell analysis

poly_R <- R %>% polychoric(.)   # matriz de correlações policóricas para análise paralela.
rho_R <- poly_R$rho # guardando apenas o Rho de Spearman da matriz de correlações policóricas
scree_R<-scree(rho_R)

pa_R <- fa.parallel(rho_R, n.obs=2000, fa="fa") # análise paralela sobre a matriz de correlações policóricas
pa_R$fa.values #mostra os eigenvalues com os dados experimentais
pa_R$fa.sim #mostra os eigenvalues com os dados simulados

# OBS.: a análise paralela indicou 6 fatores, o scree-plot 3 ou 4 fatores,
# e pelo critério de Kaiser-Guttman (eigenvalue > 1), formariam-se quatro fatores.
# Assim, optou-se pela extração de 4 fatores.

# Fatores
names(R_2)
r_efa <- fa(R, nfactors = '4', cor='poly', 
            fm='wls', rotate = 'geominQ')
# nessa analise, o item 7 apresentou cargas inferiores a 0,3 em todos os fatores.

R <- R[ ,c(1:6,8:24)]

r_efa <- fa(R, nfactors = '4', cor='poly', 
            fm='wls', rotate = 'geominQ')

r_efa$e.values
# resultados em tabelas
names (r_efa)
r_efa$loadings

#install.packages("DT")  # pacote para fazer tabelas.

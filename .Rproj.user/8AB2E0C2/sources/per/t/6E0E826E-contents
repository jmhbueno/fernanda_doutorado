### Organizacao dos dados ---
names(riasec_banco_final)
view(riasec_banco_final)
view(riasec_2)
riasec_banco_final <- riasec_banco_final %>% slice(-1471)
riasec_banco_final <- riasec_banco_final %>% select(-c(160:182))


riasec_banco_final <- riasec_banco_final %>% rename(
  Trabalho = 'A qual das áreas abaixo o seu trabalho se relaciona predominantemente?')

count(riasec_banco_final,Trabalho)
names(riasec_banco_final)
riasec_banco_final <- riasec_banco_final %>% mutate(Front_end = if_else(Trabalho == 'Desenvolvimento front-end',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(Back_end = if_else(Trabalho == 'Desenvolvimento back-end',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(Mobile = if_else(Trabalho == 'Desenvolvimento Mobile',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(Full_stack = if_else(Trabalho == 'Desenvolvimento Full-Stack',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(Banco_Dados = if_else(Trabalho == 'Banco de Dados',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(Big_data = if_else(Trabalho == 'Big data',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(ciencia_dados = if_else(Trabalho == 'Ciência de dados',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(informacao = if_else(Trabalho == 'Segurança da informação',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(computadores = if_else(Trabalho == 'Redes de computadores',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(Nuvem = if_else(Trabalho == 'Nuvem',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(UX = if_else(Trabalho == 'UX/Experiência do usuário',1,0))
riasec_banco_final <- riasec_banco_final %>% mutate(Classificacao_Erica_Area = `Classificação Érica - Área`)
riasec_banco_final <- riasec_banco_final %>% mutate(Classificacao_Erica_Grupo = `Classificação Érica - Grupo`)

count(riasec_banco_final,UX)

names(riasec_banco_final)
count(riasec_banco_final,Classificacao_Erica_Grupo)
## retirada da coluna 25 e 26 feita pela Erika e colocada ao final do banco.
riasec_banco_final <- riasec_banco_final %>% select(-25,-26)
teste$y[teste$y == '1'] <- '9'
riasec_banco_final$Classificacao_Erica_Grupo[riasec_banco_final$Classificacao_Erica_Grupo=="Gestão de Projetos"] <- "Gestão de projetos"
riasec_banco_final$Classificacao_Erica_Grupo[riasec_banco_final$Classificacao_Erica_Grupo=="UX/Experiência do Usuário"] <- "UX/Experiência do usuário"

names(riasec_banco_final)
riasec_banco_final <- riasec_banco_final %>% rename(Trabalha_informacao_superior = c(23))
riasec_banco_final <- riasec_banco_final %>% rename(Trabalha_informacao_tecnico = c(13))

## Alteracao do nome de banco para facilitar a analise
# Ausencia do Ti_tec nesse banco, alterando a posicao da coluna 24 para a coluna 23.

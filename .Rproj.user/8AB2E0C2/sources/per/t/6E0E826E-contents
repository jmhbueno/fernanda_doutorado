rely_I2<-I %>% select(1:6) %>% omega(digits=3,poly=TRUE)
rely_I3<-I %>% select(7,8,10,12,13) %>% omega(digits=3,poly=TRUE)

rely_I1$alpha %>% round(digits=2)
rely_I1$omega.tot %>% round(digits=2)
rely_I2$alpha %>% round(digits=2)
rely_I2$omega.tot %>% round(digits=2)
rely_I3$alpha %>% round(digits=2)
rely_I3$omega.tot %>% round(digits=2)


# Repeticao das sintaxes anteriores mas com as facetas no lugar dos fatores


logit_g_educa <- glm(ti_g_educa ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_gesta <- glm(ti_g_gesta ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_userx <- glm(ti_g_userx ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_supma <- glm(ti_g_supma ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_dados <- glm(ti_g_dados ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_infra <- glm(ti_g_infra ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)
logit_g_desen <- glm(ti_g_desen ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, family = binomial('logit'), data=Grande_banco_riasec, na.action =na.omit)

# Unir os NAs e N?o em um so valor numerico = 0
#Todos que disseram sim em algumas das etapas viram 1

Grande_banco_riasec$Trabalha_informacao_tecnico[is.na(Grande_banco_riasec$Trabalha_informacao_tecnico)] <- 0
Grande_banco_riasec$Trabalha_informacao_tecnico[Grande_banco_riasec$Trabalha_informacao_tecnico == 'Não'] <- 0
Grande_banco_riasec$Trabalha_informacao_tecnico[Grande_banco_riasec$Trabalha_informacao_tecnico == 'Sim'] <- 1

Grande_banco_riasec$Trabalha_informacao_superior[is.na(Grande_banco_riasec$Trabalha_informacao_superior)] <- 0
Grande_banco_riasec$Trabalha_informacao_superior[Grande_banco_riasec$Trabalha_informacao_superior == 'Não'] <- 0
Grande_banco_riasec$Trabalha_informacao_superior[Grande_banco_riasec$Trabalha_informacao_superior == 'Sim'] <- 1
# Transformar as variaveis em numerico
Grande_banco_riasec$Trabalha_informacao_superior <- as.numeric(as.character(Grande_banco_riasec$Trabalha_informacao_superior))
Grande_banco_riasec$Trabalha_informacao_tecnico <- as.numeric(as.character(Grande_banco_riasec$Trabalha_informacao_tecnico))
# Criar uma variavel nova com o nome *ti* em que se soma trabalha superior e tecnico
# Por haver possibilidade de marcar sim em apenas uma das opcoes, a soma nao resultara em valor 2
# Dessa forma, quem marcar 1 em alguma das opcoes ficara com resultado final de 1
# Essa nova variavel tera entao apenas sujeitos que trabalham com informatica seja no superior ou tecnico
Grande_banco_riasec <- Grande_banco_riasec %>% mutate(ti = Trabalha_informacao_superior + Trabalha_informacao_tecnico)

count(Grande_banco_riasec, Trabalha_informacao_superior)
count(Grande_banco_riasec, Trabalha_informacao_tecnico)
count(Grande_banco_riasec, ti)


## Ajustes
## alteracao de nome das variaveis de Erica
Grande_banco_riasec <- Grande_banco_riasec %>% rename(
  ti_area = 'Classificacao_Erica_Area')
Grande_banco_riasec <- Grande_banco_riasec %>% rename(
  ti_grupo = 'Classificacao_Erica_Grupo')

names(Grande_banco_riasec)

view(count(Grande_banco_riasec, ti_area))
view(count(Grande_banco_riasec, ti_grupo))
view(count(Grande_banco_riasec, Trabalho))
unique(Grande_banco_riasec$ti_area)
count(Grande_banco_riasec, ti_area)
Grande_banco_riasec$ti_area[Grande_banco_riasec$ti_area == '\tCiência de Dados'] <- 'Ciência de Dados'
Grande_banco_riasec$ti_area[Grande_banco_riasec$ti_area == 'Ciência de dados'] <- 'Ciência de Dados'
Grande_banco_riasec$ti_area[Grande_banco_riasec$ti_area == 'UX/Experiência do usuário'] <- 'UX/Experiência do Usuário'
Grande_banco_riasec$ti_area[Grande_banco_riasec$ti_area == 'Gestão de Projetos'] <- 'Gestão de projetos'

Grande_banco_riasec$ti_grupo[Grande_banco_riasec$ti_grupo == 'UX/Experiência do usuário'] <- 'UX/Experiência do Usuário'
Grande_banco_riasec$ti_grupo[Grande_banco_riasec$ti_grupo == 'Gestão de Projetos'] <- 'Gestão de projetos'


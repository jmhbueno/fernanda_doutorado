# EFA DE SEGUNDA ORDEM===========================================================

escores <- riasec_3 %>% select (150:166) 
names(escores)
escores %>% fa.parallel()
scree_segunda_ordem<-scree(escores)

riasec_segunda_ordem <- escores %>% fa(nfactors = 6, fm='wls', 
                                       rotate = 'geominQ', cor = 'cor', use = "pairwise")

riasec_segunda_ordem$e.values

kable(riasec_segunda_ordem$loadings[1:17, 1:6],digits = 2)

riasec_segunda_ordem$e.values
names(escores)
rely_R<- escores %>% select(1:4) %>% psych::alpha()
rely_I<- escores %>% select(5,7) %>% psych::alpha()
rely_A<- escores %>% select(8:9) %>% psych::alpha()
rely_S<- escores %>% select(6,10,11) %>% psych::alpha()
rely_E<- escores %>% select(12:15) %>% psych::alpha()
rely_C<- escores %>% select(16:17) %>% psych::alpha()

rely_R$total
rely_I$total
rely_A$total
rely_S$total
rely_E$total
rely_C$total

riasec_3$R<- escores %>% select(1:4) %>% rowMeans()

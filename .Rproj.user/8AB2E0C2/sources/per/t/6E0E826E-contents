#==============================
#==============================
count_cursos <- net3 %>% count(Curso) %>% as.data.frame()
write_excel_csv2(count_cursos,"cursos.csv")
count_cursos2 <- net3 %>% count(Curso) # contagem dos cursos c nomes modificados
count_cursos3 <- count_cursos2 %>% filter(n>=10) # selecao dos cursos com nomes modificados cujo N é maior que 10
vetor_cursos <- count_cursos3$Curso %>% as.vector() # salvar a lista de curso como vetor


net4 <- net3 %>% filter(Curso %in% vetor_cursos) # este comando FUNCIONOU... filtra df net3 pelos nomes dos cursos que estão em vetor_cursos
net4 <- net4 %>% select(6,149:172,209) # seleção das variáveis de interesse para redes
net4 <- net4 %>% na.exclude(Curso) # comando para excluir NA
count(net4,Curso) %>% view()
# net4 é um dataframe que contem os cursos com nomes padronizados e filtrados por n>10

names(net4)
x <- apply(net4[ ,c(7,10,13,17,22,25)], 1, which.max) # Identificar maior valor entre os escores RIASEC e retornar número correspondente à coluna RIASEC (1 a 6)

net4 <- net4 %>% mutate(maior=x) # salvar x como coluna de net4
names(net4)

# Trasnformação dos valores das colunas nas letras RIASEC, respectivamente.
net4$maior_cod[net4$maior == 1] <- "R"
net4$maior_cod[net4$maior == 2] <- "I"
net4$maior_cod[net4$maior == 3] <- "A"
net4$maior_cod[net4$maior == 4] <- "S"
net4$maior_cod[net4$maior == 5] <- "E"
net4$maior_cod[net4$maior == 6] <- "C"

names(net4)

#================================================================================
# SINTAXE QUE FUNCINOU PARA PLOTAR A REDE COM AS PORCENTAGENS
#================================================================================

df_arestas <- net4 %>% select(26,28)

cursos_freq <- as.data.frame(table(df_arestas)) #somar as frequências.
cursos_freq_rmzero <- subset(cursos_freq, Freq>0) #retirar frequências inferiores a 10


g <- graph_from_data_frame(d=df_arestas,directed = FALSE) # cada aluno é uma aresta
#g <- graph_from_data_frame(d=porc,directed = FALSE) # cada curso é um vértice
plot(g)

porc <- df_arestas %>% 
  group_by(Curso) %>% 
  count(maior_cod) %>% 
  mutate(porc = n/sum(n)*100) %>% 
  view()



E(g)$weight <- E(g)$porc
plot(g)

plot(g, 
     edge.width = abs(E(g)$weight)/8)



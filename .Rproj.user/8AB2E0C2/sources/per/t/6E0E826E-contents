Grande_banco_riasec <- Grande_banco_riasec %>% 
  mutate(cursos_selecionados = if_else(Curso=="adm",1,
                                       if_else(Curso=="ADS",2,
                                               if_else(Curso=="Psicologia",3,
                                                       if_else(Curso=="Gestão de Recursos Humanos",4,0)))))

count(Grande_banco_riasec,cursos_selecionados)

#Criacao do banco com apenas pessoas dos cursos_selecionados
Grande_banco_riasec_anova <- Grande_banco_riasec %>% filter(cursos_selecionados>=1)
Grande_banco_riasec_anova %>% count(cursos_selecionados)
names(Grande_banco_riasec_anova)

Grande_banco_riasec_anova <- Grande_banco_riasec_anova %>% mutate(curso_psi = if_else(cursos_selecionados==3,1,0))
Grande_banco_riasec_anova <- Grande_banco_riasec_anova %>% mutate(curso_adm = if_else(cursos_selecionados==1,1,0))
Grande_banco_riasec_anova <- Grande_banco_riasec_anova %>% mutate(curso_ads = if_else(cursos_selecionados==2,1,0))
Grande_banco_riasec_anova <- Grande_banco_riasec_anova %>% mutate(curso_grh = if_else(cursos_selecionados==4,1,0))

Grande_banco_riasec_anova %>% count(curso_psi)
Grande_banco_riasec_anova %>% count(curso_adm)
Grande_banco_riasec_anova %>% count(curso_ads)
Grande_banco_riasec_anova %>% count(curso_grh)

# Analises de regressao

logit_psi <- glm(curso_psi ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, 
                 family = binomial('logit'), 
                 data=Grande_banco_riasec_anova, 
                 na.action =na.omit)

logit_adm <- glm(curso_adm ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, 
                 family = binomial('logit'), 
                 data=Grande_banco_riasec_anova, 
                 na.action =na.omit)

logit_ads <- glm(curso_ads ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, 
                 family = binomial('logit'), 
                 data=Grande_banco_riasec_anova, 
                 na.action =na.omit)

logit_grh <- glm(curso_grh ~ R1+R2+R3+R4+I1+I2+A1+A2+S1+S2+S3+E1+E2+E3+E4+C1+C2, 
                 family = binomial('logit'), 
                 data=Grande_banco_riasec_anova, 
                 na.action =na.omit)

summary(logit_psi)
summary(logit_adm)
summary(logit_ads)
summary(logit_grh)

coef(logit_psi) %>% exp() 
coef(logit_adm) %>% exp()
coef(logit_ads) %>% exp()
coef(logit_grh) %>% exp()

names(Grande_banco_riasec_anova)

Grande_banco_riasec_anova %>% group_by(Sexo,curso_admin) %>% summarise(n=n()) %>% kable()

Grande_banco_riasec %>% group_by(curso_admin) %>% count(Sexo) %>% mutate(porc = n/sum(n)*100) %>% kable()#clacular porcentagem
Grande_banco_riasec %>% group_by(curso_adsis) %>% count(Sexo) %>% mutate(porc = n/sum(n)*100) %>% kable()#clacular porcentagem
Grande_banco_riasec %>% group_by(curso_psico) %>% count(Sexo) %>% mutate(porc = n/sum(n)*100) %>% kable()#clacular porcentagem
Grande_banco_riasec %>% group_by(curso_gerhu) %>% count(Sexo) %>% mutate(porc = n/sum(n)*100) %>% kable()#clacular porcentagem

Grande_banco_riasec_anova %>% group_by(cursos_selecionados) %>% summarise(n=n(), idade = mean(Idade), desvp = sd(Idade)) %>% kable()
count(Grande_banco_riasec_anova)

write_excel_csv2(Grande_banco_riasec,"riasec_completo_atualizado.csv")


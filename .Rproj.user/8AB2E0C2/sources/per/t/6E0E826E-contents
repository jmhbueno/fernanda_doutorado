# ESTUDO 2 - ANALISE FATORIAL CONFIRMATORIA (N=609)====

cfa_model_reduzido_geral <- 
'R_1 =~ y14+y24+y16+y23
R_2 =~ y1+y4+y6+y22
R_3 =~ y9+y11+y8+y12
R_4 =~ y2+y3+y5
R_Geral =~ R_1+R_2+R_3+R_4
I_1 =~ y97+y33+y102+y103
I_2 =~ y31+y32+y37+y34
I_Geral =~ I_1+I_2
A_1 =~ y43+y38+y39+y45
A_2 =~ y42+y41+y48+y49
A_Geral =~ A_1+A_2
S_1 =~ y60+y57+y59+y62
S_2 =~ y53+y54+y51+y55
S_3 =~ y25+y28+y27+y26
S_Geral =~ S_1+S_2+S_3
E_1 =~ y68+y70+y69+y65
E_2 =~ y80+y78+y77+y79
E_3 =~ y76+y104+y67+y75
E_4 =~ y72+y73+y74+y71
E_Geral =~ E_1+E_2+E_3+E_4
C_1 =~ y83+y86+y85+y84
C_2 =~ y90+y91+y92+y96
C_Geral =~ C_1+C_2'
names(RIASEC_ITENS_CFA_2)
cfa_reduzido_geral_results <- cfa_model_reduzido_geral %>% cfa(data=RIASEC_ITENS_CFA, estimator='dwls', 
                                                               orthogonal=FALSE, ordered=TRUE)

resultados_cfa_geral <- summary(cfa_reduzido_geral_results, standardized=TRUE, fit.measures=TRUE, modindices=FALSE)

parameterEstimates(cfa_reduzido_geral_results, standardized = TRUE) %>% filter(op=='=~') %>% 
  select('Dimens?o'=lhs, Item=rhs, B=est, SE=se, Z=z, 'p-valor'=pvalue, Beta=std.all)

semPaths(cfa_reduzido_geral_results, 
         whatLabels = "std",
         intercepts = FALSE,
         residuals = FALSE, 
         edge.label.cex = 0.9,
         layout = 'tree',
         rotation = 2,
         edge.color = 'purple',
         color = list(lat='deepskyblue', man = 'grey'),
         normalize=TRUE)


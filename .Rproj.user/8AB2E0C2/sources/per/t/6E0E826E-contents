#ESTUDO 2 - ANALISE FATORIAL CONFIRMATORIA======================================
# PREPARACAO DOS DADOS PARA CFA (N=609) ======
# Dados reais -
## Foram criados duas copias do riasec_4, o RIASEC_ITENS_CFA_2 foi criado com a manuntencao das colunas originais
## o RIASEC_ITENS_CFA_2 tem a finalidade de auxiliar a consulta da POSICAO dos itens para montar o modelo da CFA
## o RIASEC_ITENS_CFA foi criado com a substituicao dos nomes das colunas para facilitar a montagem da CFA
names(riasec_4)
RIASEC_ITENS_CFA_2 <- riasec_4 %>% select(42:146)

names(RIASEC_ITENS_CFA_2)
names(RIASEC_ITENS_CFA)

RIASEC_ITENS_CFA <- RIASEC_ITENS_CFA_2 %>%  rename_all(list(~c('y1',
                                                               'y2',
                                                               'y3',
                                                               'y4',
                                                               'y5',
                                                               'y6','y7','y8','y9',
                                                               'y10','y11','y12','y13',
                                                               'y14','y15','y16','y17',
                                                               'y18','y19','y20','y21',
                                                               'y22','y23','y24','y25',
                                                               'y26','y27','y28','y29',
                                                               'y30','y31','y32','y33',
                                                               'y34','y35','y36','y37',
                                                               'y38','y39','y40','y41',
                                                               'y42','y43','y44','y45','y46',
                                                               'y47','y48','y49','y50','y51',
                                                               'y52','y53','y54','y55','y56',
                                                               'y57','y58','y59','y60','y61','y62',
                                                               'y63','y64','y65','y66','y67','y68',
                                                               'y69','y70','y71','y72','y73','y74',
                                                               'y75','y76','y77','y78','y79','y80',
                                                               'y81','y82','y83','y84','y85','y86',
                                                               'y87','y88','y89','y90','y91','y92',
                                                               'y93','y94','y95','y96','y97','y98','y99',
                                                               'y100','y101','y102','y103','y104','y105')))



names(RIASEC_ITENS_CFA)

R_2 <- RIASEC_ITENS_CFA %>% select(1:6,8:24)
I_2 <- RIASEC_ITENS_CFA %>% select(25:37,97:103,105)
A_2 <- RIASEC_ITENS_CFA %>% select(38:50)
#S_2 <- RIASEC_ITENS_CFA %>% select(1:5,7:14) #PROBLEMA
S_2 <- RIASEC_ITENS_CFA %>% select(51:55,57:64) 
E_2 <- RIASEC_ITENS_CFA %>% select(65:82,104)
C_2 <- RIASEC_ITENS_CFA %>% select(83:96)


# FIDEDIGNIDADE Realistico ====

fator_R1<-R_2 %>% select(13,15,22,23) %>% omega(poly=TRUE)
fator_R2<-R_2 %>% select(1,4,6,21) %>% omega(poly=TRUE)
fator_R3<-R_2 %>% select(7,8,10,11) %>% omega(poly=TRUE)
fator_R4<-R_2 %>% select(2,3,5) %>% omega(nfactors=2,poly=TRUE)
fator_R_reduzido <- R_2 %>% select (1:8,10,11,13,15,21:23) %>% omega(poly=TRUE)

fator_R1$alpha %>% round(digits=2)
fator_R1$omega.tot %>% round(digits=2)
fator_R2$alpha %>% round(digits=2)
fator_R2$omega.tot %>% round(digits=2)
fator_R3$alpha %>% round(digits=2)
fator_R3$omega.tot %>% round(digits=2)
fator_R4$alpha %>% round(digits=2)
fator_R4$omega.tot %>% round(digits=2)
fator_R_reduzido$alpha %>% round(digits=2)
fator_R_reduzido$omega.tot %>% round(digits=2)

# FIDEDIGNIDADE Investigativo====

fator_I1<-I_2 %>% select(9,14,19,20) %>% omega(poly=TRUE)
#fator_I2<-I_2 %>% select(1:4) %>% omega() # Com base na EFA_SO, esse fator passou a ser o S3
fator_I2<-I_2 %>% select(7,8,10,13) %>% omega(nfactors=2,poly=TRUE) # Em decorrência da trasnferência 
# do fator anterior, I3 passou a ser I2
fator_I_reduzido <- I_2 %>% select (7:10,13,14,19,20) %>% omega(nfactors=2,digits=3,poly=TRUE)

fator_I1$alpha %>% round(2)
fator_I1$omega.tot  %>% round(2)
fator_I2$alpha %>% round(2)
fator_I2$omega.tot %>% round(2)
fator_I_reduzido$alpha %>% round(2)
fator_I_reduzido$omega.tot %>% round(2)


